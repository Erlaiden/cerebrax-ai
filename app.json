{
  "expo": {
    "name": "CerebraX AI",
    "slug": "cerebrax-ai",
    "scheme": "cerebrax-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": { "image": "./assets/images/splash-icon.png", "resizeMode": "contain", "backgroundColor": "#000000" },
    "updates": { "fallbackToCacheTimeout": 0 },
    "assetBundlePatterns": ["**/*"],
    "ios": { "supportsTablet": false },
    "android": {
      "package": "com.erlaiden.cerebraxai",
      "adaptiveIcon": { "foregroundImage": "./assets/images/android-icon-foreground.png", "backgroundColor": "#000000" }
    },
    "web": { "bundler": "metro", "favicon": "./assets/images/favicon.png" },
    "plugins": []
  }
}
